<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="2">
  <test thread-count="2" name="Test">
	  <parameter name="browser" value="chrome"></parameter>
	  <parameter name="url" value="https://www.amazon.in/"></parameter>
	  <parameter name="reportname" value="AmazonLogin"></parameter>
	  <parameter name="key" value="true"></parameter>
    <classes>
      <class name="testcases.AmazonLogin"/>
      <class name="testcases.AmazonShop"/>
    </classes>
    </test>
    <test  name="Signin">
	  <parameter name="browser" value="chrome"></parameter>
	  <parameter name="url" value="https://www.amazon.in/"></parameter>
	  <parameter name="reportname" value="AmazonLogin"></parameter>  
	  <parameter name="key" value="true"></parameter>
    <classes>
      <class name="testcases.SigninTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
